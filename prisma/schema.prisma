// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int    @id @default(autoincrement())
  name       String
  email      String @unique
  avatar     String
  password   String
  birth_date DateTime
  createdAt  DateTime
}

model clans {
  id         Int    @id @default(autoincrement())
  name       String @unique
  link       String
  icon       String
}


